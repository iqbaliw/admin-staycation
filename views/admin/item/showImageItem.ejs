<div class="tab-pane fade show active" id="showImageItem" role="tabpanel" aria-labelledby="show-item-tab">
  <div class="row">
  <% for( let index = 0; index < item.imageId.length; index++ ) { %>
    <div class="col-md-4">
      <div class="card shadow mb-4 mt-2">
        <div class="card-body">
          <img src="/<%= item.imageId[index].imageUrl %>" alt="<%= item.imageId[index].imageUrl %>" style="width:100%;display:block">
        </div>
      </div>
    </div>
  <% } %>
  </div>

  <a href="/admin/item" class="btn btn-warning btn-sm">Back</a>
</div>